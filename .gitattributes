# Auto detect text files and perform LF normalization
* text=auto

*.css           eol=lf  
*.df            eol=lf
*.htm           eol=lf
*.html          eol=lf
*.java          eol=lf
*.js            eol=lf
*.json          eol=lf
*.jsp           eol=lf
*.jspf          eol=lf
*.jspx          eol=lf         
*.properties    eol=lf
*.sh            eol=lf
*.tld           eol=lf
*.text          eol=lf
*.tag           eol=lf
*.tagx          eol=lf
*.xml           eol=lf 
*.yml           eol=lf

*.class         binary
*.dll           binary
*.ear           binary
*.gif           binary
*.ico           binary
*.jar           binary
*.jpg           binary
*.jpeg          binary
*.png           binary
*.so            binary
*.war           binary